           },1000)
          }else{
            toster(data.message,"red")
          }

            // You can also update the UI based on the server response
        } else {
          toster(response.statusText,"red")
        }
    } catch (error) {
      toster(error.message,"red")
        console.error('Error:', error);
    }
    } );
